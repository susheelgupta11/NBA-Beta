   <div class="container container1">
	<div class="row">
		
		
		<div class="col-sm-12">
			
			<div class="main-registeration-form">
					<!--login form -->
					<div class="register-heading1">
						<h4>Fill this and know your application form to be applied</h4>
					</div>
						<form class="form-horizontal" role="form" autocomplete="off">
							<div class="col-sm-12">
								<div class="form-group">
								<h4 class="register-form-details-heading">Who Should Apply ?</h4>
								</div>
							</div>
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  1.Citizen of Indian
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  2.Indian Entities (body Corporate,associations/organization registered in india)
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  3.Non Resident of India (NRI)
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  4.Citizen of a foriegn Country
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  5.Foriegn entities (not incorporated or registered in india)
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  6.indian entities (incorporated or registered in india under any law for the time being in force, which has any non-indian participations in its share capital or management)
									  
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  7.Indian Scientists / Researcher
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  8.Goverment Institutions
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							<div class="col-sm-12">
								<div class="form-group">
								<h4 class="register-form-details-heading">What to access ?</h4>
								</div>
							</div>
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  9.Biological resources  (plants , animals and micro organisms) and not value Added Products & nnt human genetic material
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  10.Knowledge associated with the biological resources
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							
							<div class="col-sm-12">
								<div class="form-group">
								<h4 class="register-form-details-heading">For what purpose ?</h4>
								</div>
							</div>
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  11.To carry out research
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  12.To carry out bio-survey/bio-utilizations
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  13.For commercial utilization
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  14.To transfer the result of research on a biological resource to a NRI/non-indian/non-indian entity/Indian entity having non-indian participations in share capitalk or management.
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  15.To obtain IPRs in india or outside for invention or research using Indian biological resource and /or associated knowledge.
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  16.To carry out non-commercial /basic research by carrying sending the biological resources.
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  16.To carry out research urgent studies for emergency purpose like to avert epidemics , etc.outside India by carrying sending the biological resource  .
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							<div class="col-sm-12">
								<div class="form-group">
									  <label class="control-label col-sm-11" for="email">
									  16.To transfer the accessed biological resources to the the third party.
									  </label>
									  <div class="col-sm-1">
										<input type="checkbox">
									  </div>
								</div>
							</div>
							
							<div class="form-group"> 
							  
							  <div class="col-sm-12">
								<a href="registration_form2.html" class="btn  login-btin pull-right">Submit</a>
							  </div>
							</div>
							
							
						</form>
					<!--login form ends -->
